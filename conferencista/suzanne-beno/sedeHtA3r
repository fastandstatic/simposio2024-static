<script>$(document).ready(function () {
		$(document).on('mouseenter', '#conferid', function() {
				$(this).find('#img-circle-conf').css('display','none');
				$(this).find('#text-inst').css('display','none');
				
					// Detectar si es un dispositivo móvil
					if (isMobileDevice()) {
						$(this).find('#name-conf #title-name-conf h2')
							.css('color','#fff')
							.stop(true, true)
							.animate({top: '100px'}, 500);
					} else {
						$(this).find('#name-conf #title-name-conf h2')
							.css('color','#fff')
							.stop(true, true)
							.animate({top: '150px'}, 500);
					}
			});	
			 $(document).on('mouseleave', '#conferid', function() {
				$(this).find('#img-circle-conf').css('display','block');
				$(this).find('#text-inst').css('display','block');
				
					// Detectar si es un dispositivo móvil
					if (isMobileDevice()) {
						$(this).find('#name-conf #title-name-conf h2')
							.css('color','#FFA400')
							.stop(true, true)
							.animate({top: '0px'}, 500);
					} else {
						$(this).find('#name-conf #title-name-conf h2')
							.css('color','#FFA400')
							.stop(true, true)
							.animate({top: '0px'}, 500);
					}
      	});
		  // Función para detectar si es un dispositivo móvil
        function isMobileDevice() {
            return /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent);
        }
    });</script><!-- end Simple Custom CSS and JS -->
<meta name="generator" content="Elementor 3.20.2; features: e_optimized_assets_loading, e_optimized_css_loading, e_font_icon_svg, additional_custom_breakpoints, block_editor_assets_optimize, e_image_loading_optimization; settings: css_print_method-external, google_font-enabled, font_display-auto">
<style>.line {
  position: relative;
}
.line-mask {
  position: absolute;
  top: 0;
  right: 0;
  background-color: #EFF3F3;
  opacity: 0.65;
  height: 100%;
  width: 100%;
  z-index: 2;
