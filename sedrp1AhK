<script>$(document).ready(function () {
		$(document).on('mouseenter', '#conferid', function() {
				$(this).find('#img-circle-conf').css('display','none');
				$(this).find('#text-inst').css('display','none');
				
					// Detectar si es un dispositivo móvil
					if (isMobileDevice()) {
						$(this).find('#name-conf #title-name-conf h2')
							.css('color','#fff')
							.stop(true, true)
							.animate({top: '100px'}, 500);
					} else {
						$(this).find('#name-conf #title-name-conf h2')
							.css('color','#fff')
							.stop(true, true)
							.animate({top: '150px'}, 500);
					}
			});	
			 $(document).on('mouseleave', '#conferid', function() {
				$(this).find('#img-circle-conf').css('display','block');
				$(this).find('#text-inst').css('display','block');
				
					// Detectar si es un dispositivo móvil
					if (isMobileDevice()) {
						$(this).find('#name-conf #title-name-conf h2')
							.css('color','#FFA400')
							.stop(true, true)
							.animate({top: '0px'}, 500);
					} else {
						$(this).find('#name-conf #title-name-conf h2')
							.css('color','#FFA400')
							.stop(true, true)
							.animate({top: '0px'}, 500);
					}
      	});
		  // Función para detectar si es un dispositivo móvil
        function isMobileDevice() {
            return /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent);
        }
    });</script><!-- end Simple Custom CSS and JS -->
